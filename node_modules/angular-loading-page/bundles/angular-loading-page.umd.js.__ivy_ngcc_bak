(function (global, factory) {
    typeof exports === 'object' && typeof module !== 'undefined' ? factory(exports, require('@angular/core'), require('@angular/common'), require('@angular/common/http'), require('@angular/router'), require('rxjs'), require('rxjs/operators')) :
    typeof define === 'function' && define.amd ? define('angular-loading-page', ['exports', '@angular/core', '@angular/common', '@angular/common/http', '@angular/router', 'rxjs', 'rxjs/operators'], factory) :
    (global = typeof globalThis !== 'undefined' ? globalThis : global || self, factory(global['angular-loading-page'] = {}, global.ng.core, global.ng.common, global.ng.common.http, global.ng.router, global.rxjs, global.rxjs.operators));
}(this, (function (exports, core, common, http, router, rxjs, operators) { 'use strict';

    var RouteInterceptorService = /** @class */ (function () {
        /**
         * @method constructor
         */
        function RouteInterceptorService(router) {
            var _this = this;
            this.router = router;
            this.isPendingRouteSubject = new rxjs.Subject();
            if (this.router) {
                router.events.subscribe(function (event) {
                    _this.routerInterceptor(event);
                });
            }
        }
        Object.defineProperty(RouteInterceptorService.prototype, "isPendingRoute", {
            /**
             * [isPendingRoute description]
             * @method isPendingRoute
             * @return [description]
             */
            get: function () {
                return this.isPendingRouteSubject.asObservable();
            },
            enumerable: false,
            configurable: true
        });
        /**
         * [routerInterceptor description]
         * @method routerInterceptor
         * @param event [description]
         */
        RouteInterceptorService.prototype.routerInterceptor = function (event) {
            if (event instanceof router.NavigationStart) {
                this.onRouteStart();
            }
            if (event instanceof router.NavigationEnd || event instanceof router.NavigationCancel || event instanceof router.NavigationError) {
                this.onRouteDone();
            }
        };
        /**
         * [onRequestStart description]
         * @method onRequestStart
         */
        RouteInterceptorService.prototype.onRouteStart = function () {
            this.isPendingRouteSubject.next(true);
        };
        /**
         * [onRequestDone description]
         * @method onRequestDone
         */
        RouteInterceptorService.prototype.onRouteDone = function () {
            this.isPendingRouteSubject.next(false);
        };
        return RouteInterceptorService;
    }());
    RouteInterceptorService.decorators = [
        { type: core.Injectable }
    ];
    RouteInterceptorService.ctorParameters = function () { return [
        { type: router.Router, decorators: [{ type: core.Optional }] }
    ]; };

    var HttpObservableService = /** @class */ (function () {
        function HttpObservableService() {
            this.pendingRequestsCounter = 0;
            this.isPendingRequestsSubject = new rxjs.Subject();
        }
        Object.defineProperty(HttpObservableService.prototype, "isPendingRequests", {
            /**
             * [isPendingRequests description]
             * @method isPendingRequests
             * @return [description]
             */
            get: function () {
                return this.isPendingRequestsSubject.asObservable();
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(HttpObservableService.prototype, "pendingRequests", {
            /**
             * [pendingRequests description]
             * @method pendingRequests
             * @return [description]
             */
            get: function () {
                return this.pendingRequestsCounter;
            },
            enumerable: false,
            configurable: true
        });
        /**
         * [onRequestStart description]
         * @method onRequestStart
         */
        HttpObservableService.prototype.onRequestStart = function () {
            this.pendingRequestsCounter++;
            if (this.pendingRequestsCounter === 1) {
                this.isPendingRequestsSubject.next(true);
            }
        };
        /**
         * [onRequestDone description]
         * @method onRequestDone
         */
        HttpObservableService.prototype.onRequestDone = function () {
            this.pendingRequestsCounter--;
            if (this.pendingRequestsCounter === 0) {
                this.isPendingRequestsSubject.next(false);
            }
        };
        return HttpObservableService;
    }());
    HttpObservableService.decorators = [
        { type: core.Injectable }
    ];

    var HttpInterceptorService = /** @class */ (function () {
        /**
         * @method constructor
         * @param injector [description]
         */
        function HttpInterceptorService(injector) {
            this.injector = injector;
            this.httpObservableService = injector.get(HttpObservableService);
        }
        /**
         * @method intercept
         * @param request [description]
         * @param next [description]
         */
        HttpInterceptorService.prototype.intercept = function (request, next) {
            var _this = this;
            this.httpObservableService.onRequestStart();
            return next.handle(request).pipe(operators.finalize(function () {
                _this.httpObservableService.onRequestDone();
            }));
        };
        return HttpInterceptorService;
    }());
    HttpInterceptorService.decorators = [
        { type: core.Injectable }
    ];
    HttpInterceptorService.ctorParameters = function () { return [
        { type: core.Injector }
    ]; };
    function HttpInterceptorServiceFactory(injector) {
        return new HttpInterceptorService(injector);
    }

    /*
     * mkLoadingPage Directive
