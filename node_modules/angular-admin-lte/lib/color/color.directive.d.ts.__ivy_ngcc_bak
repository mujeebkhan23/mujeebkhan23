import { Renderer2, ElementRef } from '@angular/core';
import { ColorService } from './color.service';
import { Colors } from './color.definition';
export declare class BackgroundColorDirective {
    private elementRef;
    private renderer2;
    private colorService;
    condition: boolean;
    set setPrefix(prefix: string);
    property: string;
    set setColor(color: Colors | string | undefined);
    private prefix;
    private color;
    constructor(elementRef: ElementRef, renderer2: Renderer2, colorService: ColorService);
}
export declare class ColorDirective {
    private elementRef;
    private renderer2;
    private colorService;
    set color(color: string | undefined);
    constructor(elementRef: ElementRef, renderer2: Renderer2, colorService: ColorService);
}
